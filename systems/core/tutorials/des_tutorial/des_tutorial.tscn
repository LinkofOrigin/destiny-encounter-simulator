[node name="Label2" type="Label" parent="DesTabContainer/WhatIsDESScreen/Panel/MarginContainer/HBoxContainer/WhatIsDES"]
custom_minimum_size = Vector2(800, 0)
layout_mode = 2
text = "Destiny Encounter Simulator is a tool for learning and practicing raid/dungeon mechanics in a safer and slower environment. In these encounters, it can be difficult to learn in the heat of the moment for any number of reasons.

The goal of DES is to be a training tool that lets you change functionality of the encounter and the mechanics so you can focus on exactly what you want to focus on. You may just want to disable a handful of mechanics so you can focus on the one you want to learn. Or you may want to repeatedly practice something until the motions become second nature."
autowrap_mode = 3
metadata/_edit_use_anchors_ = true

