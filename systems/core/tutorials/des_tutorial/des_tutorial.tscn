theme_override_constants/separation = 0
theme_override_styles/separator = SubResource("StyleBoxLine_nqtg7")

[node name="Label2" type="Label" parent="DesTabContainer/WhatIsDESScreen/Panel/MarginContainer/HBoxContainer/WhatIsDES2"]
custom_minimum_size = Vector2(800, 0)
layout_mode = 2
text = "While DES may try to simulate some of the Destiny experience, it is NOT intended to be a fully detailed or accurate copy. For example, it will not try to replicate enemies, FPS systems, or even general game feel. The focus is on encounter mechanics and systems that can be experienced in a space that is more conducive to learning and mastery. It is also extremely likely that DES will not perfectly match the rules of any given Destiny system (even outside of bugs)."
autowrap_mode = 3
metadata/_edit_use_anchors_ = true

[node name="RichTextLabel" type="RichTextLabel" parent="DesTabContainer/WhatIsDESScreen/Panel/MarginContainer/HBoxContainer/WhatIsDES2"]
layout_mode = 2
size_flags_vertical = 3
bbcode_enabled = true
text = "DES attempts to solve for example scenarios such as:
[ul]
Feeling overwhelmed and unable to absorb information in the moment
Watching a tutorial video that just doesn't quite click
Practicing a role that has limited players or requires RNG to give you that role (eg. relic bearer, forced teleport)
Focusing on a single mechanic without other distractions or mechanics
Refreshing your knowledge on an encounter without having to get a checkpoint
[/ul]"
scroll_active = false
shortcut_keys_enabled = false

