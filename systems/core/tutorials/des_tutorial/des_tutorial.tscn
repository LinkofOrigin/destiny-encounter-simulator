theme_override_font_sizes/font_size = 39
text = "Cont."
horizontal_alignment = 1

[node name="HSeparator" type="HSeparator" parent="DesTabContainer/WhatIsDESScreen/Panel/MarginContainer/HBoxContainer/WhatIsDES2"]
