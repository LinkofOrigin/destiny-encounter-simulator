config_version=5

[application]

config/name="Destiny Encounter Simulator"
config/version="0.1"
run/main_scene="res://encounters/verity/verity.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
run/max_fps=60
config/icon="res://icon.svg"

[autoload]

InputActions="*res://systems/globals/input_actions/input_actions.gd"
ControllerIcons="*res://addons/controller_icons/ControllerIcons.gd"
GlobalSignals="*res://systems/globals/global_signals.gd"
MenuManager="*res://ui/menus/menu_manager.tscn"
PlayerHudManager="*res://systems/globals/player_hud_manager/player_hud_manager.tscn"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/controller_icons/plugin.cfg")

[global_group]

Player=""

[gui]

theme/custom="res://ui/themes/des_theme.tres"

